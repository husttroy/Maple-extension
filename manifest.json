{
	"name": "ExampleCheck",
	"description": "A Chrome extension that augments Stack Overflow with API usage patterns mined from GitHub",
	"version": "2.0",
	"manifest_version": 2,
	"web_accessible_resources": ["jquery.min.js", "images/thumb-up.png", "images/thumb-down.png"],
	"content_scripts": [
		{
			"matches": ["https://stackoverflow.com/*"],
			"css": ["bootstrap.min.css", "style.css"],
			"js": ["jquery.min.js", "bootstrap.min.js", "script.js"]
		},
		{
			"matches": ["https://github.com/*"],
			"js": ["jquery.min.js", "ghscript.js"]
		}
	],
	"permissions": [
		"tabs", "storage", "http://localhost/","http://*/*", "https://*/*"
	]
}
